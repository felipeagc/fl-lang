import "core:fmt";

extern fn main() -> i32 {
    defer print("Yo\n");

    {
        defer print("later!\n");

        defer {
            print("world!\n");
        };

        print("hello ");
    }

    for (var i = 0; i < 10; i += 1) {
        defer print("Goodbye!\n");
        return 0;
        continue;
        print("Hello!\n");
    }

    return 0;
}
