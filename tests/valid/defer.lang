import "core:fmt";

fn main() {
    defer print("Yo\n");

    {
        defer print("later!\n");

        defer {
            print("world!\n");
        };

        print("hello ");
    }

    for (var i = 0; i < 10; i += 1) {
        defer print("Goodbye!\n");
        return;
        continue;
        print("Hello!\n");
    }
}
