import hello "imported.lang";
import "core:c";
import builtin "core:builtin";

extern fn main() void {
    hello.say_hello();
    var a: i32 = hello.some_const;
    var b: i32 = hello.some_var;

    // Assignment to imported global variable
    hello.some_var = 123;

    var some_var: hello.SomeType;
    some_var.field = 32;

    printf(c"Hello\n");

    if (builtin.OS == builtin.OsType.linux)
    {
        printf(c"I'm on linux!\n");
    }
}
