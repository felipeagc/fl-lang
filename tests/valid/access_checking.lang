import "core:c"

typedef Struct2 struct {
    a: i32,
};

typedef Struct struct {
    array: [2]Struct2,
};


func test() -> *Struct {
    return malloc(@size_of(Struct));
}

func main() {
    var fn: func* () -> *Struct = test;
    test().array[0].a = 123;
    fn().array[0].a = 123;
    fn()[0].array[0].a = 123;
}
