import "core:c";

typedef Struct2 struct {
    a: i32,
};

typedef Struct struct {
    array: [2]Struct2,
};


fn test() *Struct {
    return malloc(@sizeof(Struct));
}

extern fn main() void {
    var func: fn* () *Struct = test;
    test().array[0].a = 123;
    func().array[0].a = 123;
    func()[0].array[0].a = 123;
}
