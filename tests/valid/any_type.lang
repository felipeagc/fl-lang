module main

import "core:c"
import "core:fmt"
import "core:runtime"

func main() {
    {
        var a: @Any = 123;
        if (a.type_info.kind != runtime.TypeKind.Int) c.abort();
    }

    {
        var hello = 123;
        var a: @Any = hello;
        if (a.ptr != cast(*void)&hello) c.abort();
        if (a.type_info.kind != runtime.TypeKind.Int) c.abort();
    }

    {
        fmt.printf("hello % %\n", -123456, 123456);
    }
}
