import "core:c"

typedef Hello(V) struct {
    v: V
};

func add(V)(v: *Hello(V), b: V) -> V {
    return v.v + cast(V)b;
}

func other(V)(v: *Hello(V), b: V) -> V {
    return v.v + add(V)(v, 2);
}

func main() {
    var h = Hello(i32){321};
    var a = add(i32)(&h, 123);
    add(i32)(&h, 123);
    other(i32)(&h, 123);
    printf(c"%d\n", a);
}
