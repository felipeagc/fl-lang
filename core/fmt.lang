import c "core:c";
import os "core:os";

pub func print(str: [_]char) {
    var buf: [_]byte = void;
    buf.ptr = cast(*byte)str.ptr;
    buf.len = str.len;
    os.write_to_stream(os.get_stdout(), buf);
}

pub func eprint(str: [_]char) {
    var buf: [_]byte = void;
    buf.ptr = cast(*byte)str.ptr;
    buf.len = str.len;
    os.write_to_stream(os.get_stderr(), buf);
}
